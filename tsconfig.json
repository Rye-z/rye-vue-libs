{
  "compilerOptions": {
  "baseUrl": "./",
    "paths": {
    "@/*": ["src/*"],
      "@resource/*": ["src/resource/*"]
  },
  "target": "esnext",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true
},
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.d.js",
    "auto-imports.d.ts"
  ],
  "exclude": ["node_modules", ".idea", ".eslintrc.js"]
}
